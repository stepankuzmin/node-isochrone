{
  "name": "isochrone",
  "version": "2.0.1",
  "description": "isochrone map library",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "homepage": "https://github.com/stepankuzmin/node-isochrone",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "docs": "documentation build index.js -o docs -f html --github --name isochrone",
    "lint": "eslint .",
    "release": "standard-version",
    "test": "node test/index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engine-strict": true,
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "isochrone",
    "drivetime",
    "walktime",
    "geojson",
    "polygon",
    "osrm"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/stepankuzmin/node-isochrone.git"
  },
  "bugs": {
    "url": "https://github.com/stepankuzmin/node-isochrone/issues"
  },
  "dependencies": {
    "@turf/bbox": "4.5.2",
    "@turf/destination": "4.5.2",
    "@turf/helpers": "4.5.2",
    "@turf/point-grid": "4.5.2",
    "concaveman": "1.1.1",
    "geojson-rewind": "0.2.0",
    "turf-deintersect": "1.0.1"
  },
  "peerDependencies": {
    "osrm": "^5.0"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "2.0.1",
    "@turf/area": "4.5.2",
    "@turf/intersect": "4.5.2",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "documentation": "4.0.0-rc.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "osrm": "5.9.0",
    "standard-version": "4.2.0",
    "tape": "4.7.0"
  }
}
