{
  "name": "isochrone",
  "version": "3.0.2",
  "description": "isochrone map library",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "homepage": "https://github.com/stepankuzmin/node-isochrone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/stepankuzmin/node-isochrone.git"
  },
  "bugs": {
    "url": "https://github.com/stepankuzmin/node-isochrone/issues"
  },
  "keywords": [
    "isochrone",
    "drivetime",
    "walktime",
    "geojson",
    "polygon",
    "osrm"
  ],
  "engine-strict": true,
  "engines": {
    "node": ">=8"
  },
  "main": "index.js",
  "scripts": {
    "extract": "./node_modules/osrm/lib/binding/osrm-extract",
    "contract": "./node_modules/osrm/lib/binding/osrm-contract",
    "cz": "git-cz",
    "docs": "documentation build index.js -g --markdown-toc false -f md -o API.md",
    "lint": "eslint .",
    "release": "standard-version",
    "test": "node test/index.js"
  },
  "dependencies": {
    "@turf/helpers": "6.1.4",
    "@turf/point-grid": "6.0.1",
    "cheap-ruler": "2.5.1",
    "concaveman": "1.1.1",
    "geojson-rewind": "github:mapbox/geojson-rewind#v0.4.0",
    "turf-deintersect": "1.0.4"
  },
  "peerDependencies": {
    "osrm": "^5.20.0"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "2.1.0",
    "@turf/area": "6.0.1",
    "@turf/intersect": "6.1.3",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "documentation": "9.0.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "osrm": "5.20.0",
    "prettier": "1.15.3",
    "prettier-eslint": "8.8.2",
    "prettier-eslint-cli": "4.7.1",
    "standard-version": "4.4.0",
    "tape": "4.9.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "npm run lint",
      "git add"
    ]
  }
}
