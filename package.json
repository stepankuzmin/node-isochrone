{
  "name": "isochrone",
  "version": "3.0.0",
  "description": "isochrone map library",
  "author": "Stepan Kuzmin <to.stepan.kuzmin@gmail.com> (stepankuzmin.ru)",
  "homepage": "https://github.com/stepankuzmin/node-isochrone",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "cz": "git-cz",
    "docs": "documentation build index.js -g --markdown-toc false -f md -o API.md",
    "lint": "eslint .",
    "release": "standard-version",
    "test": "node test/index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engine-strict": true,
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "isochrone",
    "drivetime",
    "walktime",
    "geojson",
    "polygon",
    "osrm"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/stepankuzmin/node-isochrone.git"
  },
  "bugs": {
    "url": "https://github.com/stepankuzmin/node-isochrone/issues"
  },
  "dependencies": {
    "@turf/bbox": "6.0.1",
    "@turf/destination": "6.0.1",
    "@turf/helpers": "6.1.3",
    "@turf/point-grid": "6.0.1",
    "concaveman": "1.1.1",
    "geojson-rewind": "0.3.1",
    "turf-deintersect": "1.0.3"
  },
  "peerDependencies": {
    "osrm": "^5.16.4"
  },
  "devDependencies": {
    "@mapbox/geojsonhint": "2.1.0",
    "@turf/area": "6.0.1",
    "@turf/intersect": "6.1.2",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "documentation": "6.3.3",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.11.0",
    "osrm": "5.18.0",
    "standard-version": "4.3.0",
    "tape": "4.9.0"
  }
}
